{
	"key": "",
	"manifest_version": 3,
	"name": "Zen (Light)",
	"description": "The simplest light theme for Chrome",
	"author": "qurle",
	"theme": {
		// I use #RGB hexes. Translate to [R, G, B] arrays by the folliwing prompt:
		// Translate this "#RRGGBB" hex codes to [R, G, B] arrays, where R, G and B are primitive numbers from 0 to 255. be consice in convertion
		"colors": {
			"bookmark_text": "#1C1C1C",
			"button_background": "#FCFCFC",
			"control_background": "#D2D2D2",
			"frame": "#EEEEEE",
			"frame_inactive": "#FCFCFC",
			"ntp_background": "#FCFCFC",
			"ntp_header": "#F4F4F4",
			"ntp_link": "#6D6D6D",
			"ntp_text": "#1C1C1C",
			"tab_background_text": "#6D6D6D",
			"tab_background_text_inactive": "#6D6D6D",
			"tab_text": "#1C1C1C",
			"toolbar": "#FCFCFC",
			"toolbar_button_icon": "#1C1C1C",
			"omnibox_background": "#FCFCFC",
			"omnibox_text": "#6D6D6D"
		},
		"tints": {
			"buttons": [
				-1,
				0,
				0.6
			]
		},
		"properties": {
			"ntp_background_alignment": "bottom",
			"ntp_background_repeat": "no-repeat"
		}
	},
	"update_url": "https://clients2.google.com/service/update2/crx",
	"version": "1.2"
}